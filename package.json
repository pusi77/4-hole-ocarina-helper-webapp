{
  "name": "4-hole-ocarina-helper-webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "npm run type-check && npm run lint && npm run test:run && npm run build",
    "build:skip-checks": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:performance": "vitest run src/__tests__/performance",
    "test:integration": "vitest run src/__tests__/integration",
    "test:visual": "vitest run src/__tests__/visual",
    "test:accessibility": "vitest run src/__tests__/accessibility",
    "test:comprehensive": "vitest run src/__tests__/TestSuite.test.ts",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=json --reporter=html",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,css}\"",
    "type-check": "tsc --noEmit",
    "serve": "npm run build && npm run preview",
    "clean": "rm -rf dist node_modules/.vite",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:run"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
